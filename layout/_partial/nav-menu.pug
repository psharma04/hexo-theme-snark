nav.h-head.box
    .m-hdimg
        a(class="hdimg img" href="/")
            img(src="/img/maxresdefault.jpg" class="nofancybox" width="128" height="128")
        h1.ttl
            a(href="/")= config.title

    if theme.description
        p.m-desc!= theme.description

    .m-nav
        ul
            include common.pug
            - for (var i in theme.menu)
              +menu-item(theme.menu[i].directory, __(theme.menu[i].page))

            li.m-sch
                include search.pug
        div#local-search-result
        ul
            img(src='https://api.netlify.com/api/v1/badges/a7c962d4-6b4b-4657-99e5-d47f832dca09/deploy-status' alt='Deploy Status')
